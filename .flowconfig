[ignore]
.*/__snapshots__/.*

[include]
<PROJECT_ROOT>/app/javascript/
<PROJECT_ROOT>/spec/javascripts/

[libs]
./app/javascript/src/Types/libs/libdefs.js
flow-typed

[lints]

