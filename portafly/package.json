{
  "name": "portafly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@patternfly/react-core": "^3.124.1",
    "@patternfly/react-table": "^2.25.6",
    "currency.js": "^1.2.2",
    "node-polyglot": "^2.4.0",
    "react": "^16.12.0",
    "react-async": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1",
    "react-scripts": "3.3.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/node-polyglot": "^0.4.34",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "date-fns": "^2.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-with-coverage": "yarn test -- --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "@src": "<rootDir>/src/root.ts",
      "@src/(.*)": "<rootDir>/src/$1",
      "@test/(.*)": "<rootDir>/src/tests/$1"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
