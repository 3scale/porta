    "overrides": {
      "github:components/jquery@2.1.4": {
        "main": "jquery"
      },
      "github:components/jqueryui@1.11.4": {
        "main": "jquery-ui.js",
        "registry": "jspm",
        "dependencies": {
          "jquery": "*"
        }
      },
      "github:select2/select2@4.0.1": {
        "dependencies": {
          "jquery": "npm:jquery@^2.2.0"
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:c3@0.4.11": {
        "registry": "jspm",
        "format": "global",
        "dependencies": {
          "d3": "~3.5.0",
          "css": "^0.1.0"
        },
        "meta": {
          "c3.js": {
            "deps": [
              "d3",
              "./c3.css!"
            ],
            "exports": "c3",
            "format": "global"
          }
        }
      },
      "npm:d3@3.5.17": {
        "meta": {
          "d3.js": {
            "exports": "d3",
            "format": "global"
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.1": {
        "format": "amd"
      },
      "npm:lodash@4.17.0": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.15.0": {
        "jspmNodeConversion": false
      }
    }
  },
