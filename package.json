{
  "name": "porta",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/3scale/porta"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets:./app/assets/builds --no-source-map --load-path=node_modules --load-path=app/assets/stylesheets --quiet-deps && sass ./lib/developer_portal/app/assets/stylesheets:./app/assets/builds --no-source-map --load-path=node_modules --load-path=lib/developer_portal/app/assets/stylesheets --load-path=app/assets/stylesheets --quiet-deps",
    "build:css:watch": "concurrently --kill-others --names \"ADMIN,DEVPORTAL\" -c \"cyan,yellow\" \"sass ./app/assets/stylesheets:./app/assets/builds --watch --load-path=node_modules --load-path=app/assets/stylesheets --quiet-deps\" \"sass ./lib/developer_portal/app/assets/stylesheets:./app/assets/builds --watch --load-path=node_modules --load-path=lib/developer_portal/app/assets/stylesheets --load-path=app/assets/stylesheets --quiet-deps\"",
    "dev": "concurrently --kill-others --names \"WEBPACK,SASS\" -c \"blue,magenta\" \"webpack serve --config ./config/webpack/development.config.js\" \"yarn build:css:watch\"",
    "dev:webpack": "webpack serve --config ./config/webpack/development.config.js --progress",
    "dev:sass": "yarn build:css:watch",
    "build": "webpack --config ./config/webpack/production.config.js --progress",
    "install:safe": "yarn install --frozen-lockfile --check-files --prefer-offline",
    "clean": "rm -rf node_modules/ && yarn cache clean",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "engines": {
    "node": "18"
  },
