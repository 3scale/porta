<% content_for :sublayout_title do %>
  <h2>Create new Service Plan of '<%= h @service.name %>'</h2>
<% end %>

<%= semantic_form_for @plan, :url => admin_service_service_plans_path(@service) do |form| %>
  <%= render :partial => 'form', :object => form %>
  <%= form.actions do %>
    <%= form.commit_button %>
  <% end %>
<%- end %>
