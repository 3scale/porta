<%= render(:partial => 'api/metrics/prepare_slot') %>

var html = "<%= escape_javascript(render(:partial => 'pricing_rules')).html_safe %>";
var id   = "#<%= dom_id(@metric) %>_slot";
var toggle_id = '#<%= dom_id(@metric) %> td.metrics-subtable-toggle.pricing';

$('td.metrics-subtable-toggle').removeClass('selected');
$(toggle_id).addClass('selected');


jQuery(id).prepend(html);
