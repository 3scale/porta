<%= render partial: 'provider/admin/cms/tab_content_title', locals: { header: 'New partial' } %>

<%= cms_new_button_dropdown(CMS::Partial) %>

<%= cms_form_for(@page) do |f| %>
  <%= render 'form', :f => f %>
  <%= f.input :draft, :as => :codemirror, :label => false %>

  <%= f.actions do %>
    <%= f.commit_button 'Create Partial' %>
  <% end %>

<% end %>
