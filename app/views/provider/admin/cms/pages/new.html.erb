<%= render partial: 'provider/admin/cms/tab_content_title', locals: { header: 'New page' } %>

<%= cms_new_button_dropdown(CMS::Page) %>

<%= cms_form_for(@page) do |f| %>
  <%= render 'form', :f => f %>
  <%= f.input :draft, :as => :codemirror %>

  <%= f.actions do %>
    <%= f.commit_button 'Create Page' %>
  <% end %>
<% end %>
