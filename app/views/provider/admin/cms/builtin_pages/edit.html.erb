<div class="pf-c-content">
  <h1>Built-in page '<%= @page.title %>'</h1>
  <p><%= @page.description.html_safe %></p>
</div>

<%= cms_new_button_dropdown(CMS::Page) %>

<%= cms_form_for(@page, :remote => true) do |f| %>
  <div id="cms-template-fields-wrapper">
    <%= render( 'form', :f => f) %>
  </div>
  <%= render '/provider/admin/cms/content_tabs', :f => f %>
<% end %>
