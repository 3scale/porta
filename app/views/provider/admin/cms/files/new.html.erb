<%= render partial: 'provider/admin/cms/tab_content_title', locals: { header: 'Upload File' } %>

<%= cms_new_button_dropdown(CMS::File) %>

<%= cms_form_for(@file, :html => { :multipart => true }) do |f| %>
  <%= render 'form', :f => f %>

  <div class='button-bar'>
    <%= f.actions do %>
      <%= f.commit_button 'Create File' %>
    <% end %>
  </div>
<% end %>
