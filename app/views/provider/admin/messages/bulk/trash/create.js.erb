<%# TODO: when is this used? Can we delete it? %>
<%- @message_ids.each do |message_id| %>
  $("#message_" + <%= json(message_id) %>).fadeOut(function(){ $(this).remove() });
<%-end-%>

<% if @no_more_messages %>
  var text = '<%= t("provider.admin.messages.no_#{@scope}") %>';

  $('<p />').text(text).insertBefore('table#messages');
  $('table#messages, #bulk-operations, .pagination').fadeOut();
<% end %>

$.colorbox.close()
$.flash('<%= escape_javascript @notice %>')
