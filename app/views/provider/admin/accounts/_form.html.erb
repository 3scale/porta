<%= form.inputs :name => '', :class => 'inputs' do %>
  <%= form.user_defined_form(show_readonly_inputs: false) %>
  <%= time_zone_select_for_account(form) -%>
<% end %>

<%= form.actions do %>
  <% if params[:next_step] == 'credit_card' %>
    <%= form.action :submit, label: 'Save and continue with payment details' %>
  <% else %>
    <%= form.action :submit %>
  <% end %>
<% end %>
<%= javascript_include_tag 'custom_profiles_input_texts' %>
