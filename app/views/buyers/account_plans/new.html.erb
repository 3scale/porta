<h2>Create new Account Plan</h2>
<%= semantic_form_for @plan, :url => admin_buyers_account_plans_path do |form| %>
  <%= render :partial => 'form', :object => form %>
  <%= form.buttons do %>
    <%= form.button 'Create Account plan', button_html: { class: 'pf-c-button pf-m-primary' } %>
  <% end %>
<%- end %>
