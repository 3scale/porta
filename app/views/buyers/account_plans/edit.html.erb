<h1>Edit Account Plan '<%= h @plan.name %>'</h1>

<% if @plan.customized? %>
  <%= render :partial => 'api/plans/menu' %>
<% end %>

<%= semantic_form_for @plan, :url => admin_account_plan_path(@plan) do |form| %>
  <%= render :partial => 'form', :object => form %>
  <%= form.actions do %>
    <%= form.commit_button %>
  <% end %>
<%- end %>

<%= render 'api/plans/features' -%>

