<% content_for :page_header_title, "Edit Account Plan #{@plan.name}" %>

<% if @plan.customized? %>
  <% content_for :page_header_body do %>
    <%= render :partial => 'api/plans/menu' %>
  <% end %>
<% end %>

<%= semantic_form_for @plan, :url => admin_account_plan_path(@plan) do |form| %>
  <%= render :partial => 'form', :object => form %>
  <%= form.actions do %>
    <%= form.commit_button %>
  <% end %>
<%- end %>

<%= render 'api/plans/features' -%>

