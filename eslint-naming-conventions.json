{
  "rules": {
    "@typescript-eslint/naming-convention": ["error", {
      "selector": "default",
      "format": ["camelCase"]
    }, {
      "selector": "variable",
      "format": ["camelCase", "UPPER_CASE"]
    }, {
      "selector": "variable",
      "types": ["function"],
      "format": ["camelCase", "PascalCase"]
    }, {
      "selector": "parameter",
      "format": ["camelCase"],
      "leadingUnderscore": "allow"
    }, {
      "selector": "typeLike",
      "format": ["PascalCase"]
    }]
  }
}
